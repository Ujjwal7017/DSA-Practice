public class MaximumAndMinimum {
    public static void main(String[] args)
    {
        int arr[] = {12,30,10,2,89,10};
        int max = arr[0];
        int min = arr[0];
        for(int i=1;i<arr.length;i++)
        {
            if(arr[i]>max)
            {
                max = arr[i];
            }
            if(arr[i]<min)
            {
                min = arr[i];
            }
        }
        System.out.println("Max is : " + max + " Min is : " + min);
    }
}
